{
  "expo": {
    "name": "Pridepay",
    "slug": "Pridepay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "scheme": "pridepay",


    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.biglil.pridepay",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "mailto"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.biglil.pridepay",
      "config": {
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "mailto",
              "host": "biglildev@gmail.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "850c0a2d-bac8-4e08-8c54-e74d2b950a8a"
      }
    }
  }
}

{
  "expo": {
    "name": "Pridepay",
    "slug": "Pridepay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.biglil.pridepay",
      "config": {
        "googleSignIn": {
          "reservedClientId": "YOUR_RESERVED_CLIENT_ID"
        }
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["mailto"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.biglil.pridepay",
      "config": {
        "googleSignIn": {
          "apiKey": "YOUR_API_KEY"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "mailto",
              "host": "biglildev@gmail.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
